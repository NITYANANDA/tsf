<assembly>
    <id>examples</id>
    <formats>
        <format>dir</format>
        <format>zip</format>
    </formats>
    <fileSets>
        <!-- Copy Examples -->
        <fileSet>
            <directory>${basedir}/../examples</directory>
            <outputDirectory>/examples</outputDirectory>
            <excludes>
                <exclude>**/target/**</exclude>
                <exclude>**/.settings/**</exclude>
                <exclude>**/.project</exclude>
                <exclude>**/.classpath</exclude>
            </excludes>
        </fileSet>
    </fileSets>
</assembly>
